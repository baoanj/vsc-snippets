{
  "elButton": {
    "prefix": "eb",
    "body": [
      "<el-button type=\"primary\" size=\"small\" @click=\"$2\">$1</el-button>"
    ],
    "description": "baj el-button"
  },
  "elForm": {
    "prefix": "ef",
    "body": [
      "<el-form label-width=\"80px\">",
      "  <el-form-item label=\"${1:名称}\" required>",
      "    <el-input size=\"small\" v-model=\"name\"></el-input>",
      "  </el-form-item>",
      "</el-form>"
    ],
    "description": "baj el-form"
  },
  "elDialog": {
    "prefix": "ed",
    "body": [
      "<el-dialog",
      "  :visible.sync=\"visible\"",
      "  title=\"$1\"",
      "  width=\"800px\"",
      "  :modal-append-to-body=\"false\"",
      "  :close-on-click-modal=\"false\"",
      "  :close-on-press-escape=\"false\"",
      ">",
      "  $2",
      "</el-dialog>"
    ],
    "description": "baj el-dialog"
  },
  "elSelect": {
    "prefix": "es",
    "body": [
      "<el-select v-model=\"$1\" size=\"small\">",
      "  <el-option label=\"\" value=\"\"></el-option>",
      "</el-select>"
    ],
    "description": "baj el-select"
  },
  "vFor": {
    "prefix": "vf",
    "body": ["v-for=\"${1:(item, index)} in $2\" :key=\"$3\""],
    "description": "baj v-for"
  }
}
